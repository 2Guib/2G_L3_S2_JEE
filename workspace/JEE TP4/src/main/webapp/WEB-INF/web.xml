<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>JEE TP3 Servlet</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.jsp</welcome-file>
    <welcome-file>default.htm</welcome-file>
  </welcome-file-list>
  
  <servlet>
  	<servlet-name>TeamPink</servlet-name>
  	<servlet-class>difs.Team</servlet-class>
  	<init-param>
  		<param-name>color</param-name>
  		<param-value>pink</param-value>
  	</init-param>
  </servlet>
  <servlet>
  	<servlet-name>TeamBlue</servlet-name>
  	<servlet-class>difs.Team</servlet-class>
  	<init-param>
  		<param-name>color</param-name>
  		<param-value>blue</param-value>
  	</init-param>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>TeamPink</servlet-name>
  	<url-pattern>/teampink</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  	<servlet-name>TeamBlue</servlet-name>
  	<url-pattern>/teamblue</url-pattern>
  </servlet-mapping>
</web-app>