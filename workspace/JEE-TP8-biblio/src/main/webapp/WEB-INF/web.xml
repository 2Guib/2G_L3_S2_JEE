<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <listener>
  	<listener-class>difs.biblio.controller.ContextListener</listener-class>
  </listener>
  <servlet>
  	<servlet-name>IndexServlet</servlet-name>
  	<display-name>IndexServlet</display-name>
  	<description></description>
  	<servlet-class>difs.biblio.controller.IndexServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Affiche</servlet-name>
  	<display-name>Affiche</display-name>
  	<description></description>
  	<servlet-class>difs.biblio.controller.Affiche</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Ajouter</servlet-name>
  	<display-name>Ajouter</display-name>
  	<description></description>
  	<servlet-class>difs.biblio.controller.Ajouter</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Creer</servlet-name>
  	<display-name>Creer</display-name>
  	<description></description>
  	<servlet-class>difs.biblio.controller.Creer</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>IndexServlet</servlet-name>
  	<url-pattern>/IndexServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Affiche</servlet-name>
  	<url-pattern>/Affiche</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Ajouter</servlet-name>
  	<url-pattern>/Ajouter</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Creer</servlet-name>
  	<url-pattern>/Creer</url-pattern>
  </servlet-mapping>
</web-app>
